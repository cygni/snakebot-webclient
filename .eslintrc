{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "createjs": true,
    "lib": true,
    "blackhole": true
  },
  "rules": {
    "no-underscore-dangle": ["off"],
    "prefer-template": 0,
    "no-use-before-define": ["error", "nofunc"],
    "no-param-reassign": ["error", {
      "props": false
    }],
    "no-console": ["off"],
    "no-mixed-operators": ["off"],
    "padded-blocks": ["off"],
    "max-len": [2, {"code": 120, "tabWidth": 4, "ignoreUrls": true}],
    "react/jsx-indent": ["off"]
  }
}
